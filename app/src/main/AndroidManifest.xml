<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.systrack.admission">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme.NoActionBar">
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name=".EmployeeProfileActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Activity.ManageEmployeeCourseAllActivity" />
        <activity android:name=".Activity.ManageEmployeeMediumByCourseActivity" />
        <activity android:name=".Activity.ManageEmployeeDepartmentActivity" />
        <activity android:name=".Activity.ManageEmployeeDesignationActivity" />
        <activity android:name=".Activity.ManageEmployeeCategoryActivity" />
        <activity android:name=".Activity.ManageEmployeeCourseAllActivityNew" android:theme="@style/AppTheme" />
        <activity android:name=".Activity.ClassTimingBatchActivity" />
        <activity android:name=".Activity.ClassTimingSectionActivity" />
        <activity android:name=".Activity.StudentAttendanceSubjectActivity" />
        <activity android:name=".Activity.StudentAttendanceClassTimingActivity" />
        <activity android:name=".Activity.LectureAttendanceSubjectActivity" />
        <activity android:name=".EmployeeMainActivity" />
        <activity android:name=".Activity.LeaveAssignLeaveYearActivity" />
        <activity android:name=".Activity.ExamTimetableAcademicYearActivity" />
        <activity android:name=".Activity.HallArrangementExamNameActivity" />
        <activity android:name=".Activity.HallArrangementExamHallNoActivity" />
        <activity android:name=".Activity.HallArrangementExamTimeActivity" />
        <activity android:name=".Activity.SubjectAllocationEmployeeListActivity" />
        <activity android:name=".Activity.CourseMgtDivisionActivity" />
        <activity android:name=".Activity.AttendanceStatisticSubjectActivity" />
    </application>

</manifest>