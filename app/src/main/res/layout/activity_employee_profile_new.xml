<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whiteTextColor"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="250dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:background="@drawable/gradiantbackground"

            >

            <!--<ImageView-->
                <!--android:layout_width="40dp"-->
                <!--android:layout_height="40dp"-->
                <!--android:layout_margin="5dp"-->
                <!--android:src="@drawable/ic_arrow_back_white_18dp"-->
                <!--android:id="@+id/backBtn"-->
                <!--/>-->

            <LinearLayout
                android:layout_alignParentRight="true"
                android:layout_width="85dp"
                android:layout_height="30dp"
                android:background="@drawable/green_background"
                android:layout_gravity="center"
                android:layout_margin="10dp"
                android:id="@+id/backBtn">

                <com.systrack.admission.helpers.FontAwesomeTextview
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:id="@+id/back_btn"
                    android:text="&#xf04a;"
                    android:textColor="@color/whiteTextColor"
                    android:layout_gravity="center"
                    android:gravity="center"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Back"
                    android:textColor="@color/whiteTextColor"
                    android:layout_gravity="center"
                    android:layout_marginLeft="5dp"
                    android:padding="2dp"/>
            </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/user_image"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="20dp"
                android:src="@drawable/ic_username" />

            <TextView
                android:id="@+id/employee_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="5dp"
                android:text="Wiliam Snoe"
                android:textColor="#fff"
                android:textSize="17sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/employee_designation"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:text="Role name"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_margin="3dp"
                android:background="@drawable/green_background"
                android:textColor="@color/whiteTextColor"
                android:textSize="11sp" />




        </LinearLayout>
        </RelativeLayout>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="160dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:weightSum="3">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Personal"
                        android:id="@+id/txt_personal"
                        android:textSize="20sp"
                        />
                    <!--<TextView-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:text="125"-->
                        <!--android:textStyle="bold"-->
                        <!--android:textSize="20sp"-->
                        <!--android:paddingTop="10dp"-->
                        <!--android:textColor="@color/startblue"/>-->

                </LinearLayout>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/txt_guardians"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Guardians"
                        android:textSize="20sp"
                        />
                    <!--<TextView-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:text="1205"-->
                        <!--android:textStyle="bold"-->
                        <!--android:textSize="20sp"-->
                        <!--android:paddingTop="10dp"-->
                        <!--android:textColor="@color/startblue"/>-->

                </LinearLayout>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/other_info_tab"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Other Info"
                        android:textSize="20sp" />
                    <!--<TextView-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:text="360"-->
                        <!--android:textStyle="bold"-->
                        <!--android:textSize="20sp"-->
                        <!--android:paddingTop="10dp"-->
                        <!--android:textColor="@color/startblue"/>-->

                </LinearLayout>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/employee_profile_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </FrameLayout>
    </LinearLayout>

</LinearLayout>
